{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/trends",
      "dest": "server.js"
    },
    {
      "src": "/api/trends/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(us|ca|au|gb|de|fr|no|se|jp|kr|sg)",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}